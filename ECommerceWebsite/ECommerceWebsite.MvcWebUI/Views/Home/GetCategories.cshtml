@model IEnumerable<ECommerceWebsite.MvcWebUI.Entity.Category>

<ul class="list-group">
    
    @foreach (var item in Model)
    {
        <li class="list-group-item">

            <a href="/Home/List/@item.Id">
                <i class="fa-solid fa-angles-right fa-fw"></i>
                @item.CategoryName
            </a>
        </li>
    }

</ul>
